<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.kallibr44.mediawallet">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:icon="@drawable/logo_pic"
            android:label="@string/app_name"
            android:roundIcon="@drawable/logo_pic"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="GoogleAppIndexingWarning">
        <meta-data
                android:name="com.google.android.gms.ads.APPLICATION_ID"
                android:value="ca-app-pub-1102059162601993~8893057206"/>
        <activity android:name=".sendTransaction">
        </activity>
        <activity android:name=".receive_layout">
        </activity>
        <activity
                android:name=".LoginActivity"
                android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".RegistrationActivity"
                android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">
        </activity>
        <activity
                android:name=".MainActivity"
                android:label="@string/app_name"
                android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">
        </activity>
        <activity android:name=".graphic_daily"
                android:label="Earn Today"
                android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">

        </activity>
        <activity android:name=".tx_full_item"
                android:label="Detail transaction"
                android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">

        </activity>
    </application>

</manifest>